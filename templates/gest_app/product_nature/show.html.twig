{% block title %}ProductNature{% endblock %}

{% block body %}
    <h1>ProductNature</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product_nature.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ product_nature.name }}</td>
