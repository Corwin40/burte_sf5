{% extends 'base.html.twig' %}

{% block title %}ProductUnit{% endblock %}

{% block body %}
    <h1>ProductUnit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product_unit.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ product_unit.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ product_unit.slug }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ product_unit.createdAt ? product_unit.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ product_unit.updatedAt ? product_unit.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('gest_app_product_unit_index') }}">back to list</a>

    <a href="{{ path('gest_app_product_unit_edit', {'id': product_unit.id}) }}">edit</a>

    {{ include('gest_app_product_unit/_delete_form.html.twig') }}
{% endblock %}
