<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>actions</th>
    </tr>
    </thead>
    <tbody>
    {% for product_nature in product_natures %}
        <tr>
            <td>{{ product_nature.id }}</td>
            <td>{{ product_nature.name }}</td>
            <td>
                <a href="{{ path('op_gestapp_product_nature_index', {'id': product_nature.id}) }}"><i class="far fa-eye"></i></a>
                <a href="{{ path('op_gestapp_product_nature_edit', {'id': product_nature.id}) }}"><i class="far fa-edit"></i></a>
                <a href="#" class="js-modalSuppr text-danger" type="button" data-bs-toggle="modal" data-bs-target="#Suppr" data-bs-whatever="{{ product_nature.id }}">
                    <i class="bi bi-eraser text-danger"></i>
                </a>
            </td>
        </tr>
    {% else %}
        <tr>
            <td colspan="3">no records found</td>
        </tr>
    {% endfor %}
    </tbody>
</table>