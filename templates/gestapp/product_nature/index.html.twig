{% extends 'base.html.twig' %}

{% block title %}ProductNature index{% endblock %}

{% block body %}
    <h1>ProductNature index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Slug</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for product_nature in product_natures %}
            <tr>
                <td>{{ product_nature.id }}</td>
                <td>{{ product_nature.name }}</td>
                <td>{{ product_nature.slug }}</td>
                <td>{{ product_nature.createdAt ? product_nature.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ product_nature.updatedAt ? product_nature.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('op_gestapp_product_nature_show', {'id': product_nature.id}) }}">show</a>
                    <a href="{{ path('op_gestapp_product_nature_edit', {'id': product_nature.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('op_gestapp_product_nature_new') }}">Create new</a>
{% endblock %}
