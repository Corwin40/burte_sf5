{% set route_name = app.request.attributes.get('_route') %}

<!DOCTYPE html>
<html lang="fr">
    <head>
        {% block meta %}
        {{ render(controller('App\\Controller\\Webapp\\PublicController::meta')) }}
        {% endblock %}
        <meta name="viewport" content="width=device-width, initial-scale=1"/>
        <title>{% block title %}OpenGaia{% endblock %}</title>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}


    </head>
    <body>

    <main id="content">
        {% block content %}
        {% endblock %}
    </main>

</body>
</html>